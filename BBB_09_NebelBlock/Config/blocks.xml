<configs>

<append xpath="/blocks">

		<block name="Nebel_Block_00">
			<property name="Model" value="#@modfolder:Resources/Nebel_Block_00.unity3d?GameObject_Nebel_Block_00.prefab"/>
			<property name="CustomIcon" value="Nebel"/>
			<property name="MultiBlockDim" value="1,1,1"/> <!-- x,y,z breite,höhe,tiefe -->
			<property name="Material" value="Mair"/>     <!-- Klang des Blockes Mstone, Mwood, Miron -->
			<property name="MaxDamage" value="100"/>
			<property name="FuelValue" value="300"/>
			<property class="RepairItems"><property name="resourceCobblestones" value="10"/></property>
				<drop event="Destroy" name="resourceBrokenGlass" count="1,3"/>
				<drop event="Fall" name="terrDestroyedWoodDebris" count="1" prob="0.75" stick_chance="1"/>
			<property name="LightOpacity" value="1"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Collide" value="sight"/>
			<property name="ModelOffset" value="0,0,0"/>   
			<property name="PlaceEverywhere" value="true"/>
			<property name="CanPickup" value="true" />
		</block>
		
